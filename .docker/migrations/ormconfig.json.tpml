{
  "type": "postgres",
  "host":"$POSTGRES_HOST",
  "port": "$POSTGRES_PORT",
  "username": "$POSTGRES_USERNAME",
  "password": "$POSTGRES_PASSWORD",
  "database": "$POSTGRES_DATABASE",
  "migrationsTableName": "migrations",
  "migrations": ["./migrations/*.js"],
  "cli": { "migrationsDir": "./migrations" },
  "synchronize": false
}
